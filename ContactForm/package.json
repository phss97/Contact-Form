{
  "name": "contact-form",
  "version": "1.0.0",
  "scripts": {
    "install_frontend": "cd frontend && npm install",
    "install_backend": "cd backend && npm install",
    "install": "npm run install_frontend && npm run install_backend",
    "start": "react-scripts start",
    "clean": "rm -rf **/node_modules",
    "dev": "concurrently \"npm start --prefix backend\" \"npm start --prefix frontend\""
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.0"
  }
}
